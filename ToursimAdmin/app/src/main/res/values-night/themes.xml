<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ToursimAdmin" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#223ABF</item>
        <item name="colorPrimaryVariant">#0C0C0C</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="android:windowBackground">#353535</item>
        <item name="android:textColor">#FFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="change_background" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="android:backgroundTint">#201F1F</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
</resources>