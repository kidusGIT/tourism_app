<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ToursimAdmin" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#223ABF</item>
        <item name="colorPrimaryVariant">#223ABF</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#223ABF</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:windowBackground">#E3DDDD</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#223ABF</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.ToursimAdmin.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ToursimAdmin.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ToursimAdmin.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="change_background" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="android:backgroundTint">#FFFFFF</item>
        <item name="android:textColor">#1C1B1B</item>
    </style>
</resources>